<h1>Students</h1>
<a href = "/addStudent">Add Student</a>
<a href = "/">Home</a>
<table>
    <tr>
        <th>Student ID</th>
        <th>Name</th>
        <th>Age</th>
        <th>Action</th>
    </tr>
    <% students.forEach((student) => { %>
        <tr>
            <td><%= student.sid %></td>
            <td><%= student.name %></td>
            <td><%= student.age %></td>
            <td><a href = "/updateStudent">update</a></td>
        </tr>
    <% }) %>
</table>